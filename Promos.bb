;PREPARE PROMO
Function PreparePromo()
 ;ignore in rumbles
 If matchTeams=-1 Then matchPromo=0
 ;reset status
 promoLogic=1 
 promoLocked(matchPromo)=0
 promoTim=promoLength(matchPromo)
 For count=1 To 10
  promoReact(count)=0
 Next
 ;reset actors
 For cyc=1 To 3
  promoActor(cyc)=0
 Next
 ;assign new actors
 If matchPromo>0
  For cyc=1 To 3
   ;referee/secondary
   If promoRole(matchPromo,cyc)=-1
    For v=1 To no_plays
     If promoActor(cyc)=0 And pRole(v)<>0 And pRole(v)<>3 Then promoActor(cyc)=v
    Next
    If promoActor(cyc)=0 Then promoActor(cyc)=no_wrestlers
   EndIf
   ;next wrestler
   If promoRole(matchPromo,cyc)>0
    rangeA=1 : rangeB=no_plays
    If matchTeams=-1 Then rangeA=0 : rangeB=no_wrestlers-1
    For count=rangeA To rangeB
     If matchTeams=-1 Then v=no_wrestlers-count Else v=count
     If promoActor(cyc)=0 And pTeam(v)=promoRole(matchPromo,cyc) And v<>promoActor(1) And v<>promoActor(2) And v<>promoActor(3)
      promoActor(cyc)=v
     EndIf
    Next
    If promoActor(cyc)=0 And matchTeams=>0 Then promoActor(cyc)=promoRole(matchPromo,cyc)
    If promoActor(cyc)=0 And matchTeams=-1 Then promoActor(cyc)=no_wrestlers
   EndIf
  Next
 EndIf
End Function

;MAKE CHARACTER SPEAK
Function Speak(cyc,target,expression)
 ;switch focus
 camFoc=cyc : camType=1 : camOverride=0
 pFoc(cyc)=target
 For v=1 To no_plays
  If pControl(v)=0 And v<>cyc And pTeam(v)<>pTeam(cyc) Then pFoc(v)=cyc 
 Next
 ;expression
 pSpeaking(cyc)=1 : speaker=cyc
 If expression=<0 Then pEyes(cyc)=charEyes(pChar(cyc)) Else pEyes(cyc)=expression
 ;subtitle background
 If optSubtitles>0
  Color 0,0,0
  Rect rX#(0),rY#(480),rX#(800),rY#(600),1
 EndIf
End Function

;FACIAL EXPRESSIONS
Function FacialExpressions(cyc)
 ;eye expressions
 If pEyes(cyc)<>pOldEyes(cyc) And pEyes(cyc)>0 And pHidden(cyc)=0
  EntityTexture pLimb(cyc,1),tEyes(pEyes(cyc)),0,3
  pOldEyes(cyc)=pEyes(cyc)
 EndIf
 ;mouth expressions
 If pSpeaking(cyc)=1
  randy=Rnd(0,3)
  If randy=0 Then pMouth(cyc)=Rnd(-2,5)
  If pMouth(cyc)<0 Then pMouth(cyc)=0
  EntityTexture pLimb(cyc,1),tMouth(pMouth(cyc)),0,4
 EndIf
 If pSpeaking(cyc)=0 And pMouth(cyc)>0
  pMouth(cyc)=0
  EntityTexture pLimb(cyc,1),tMouth(0),0,4
 EndIf
End Function

;RESET EXPRESSIONS
Function ResetExpressions(cyc) 
 pOldEyes(cyc)=pEyes(cyc)
 pEyes(cyc)=charEyes(pChar(cyc))
 pSpeaking(cyc)=0
End Function

;BECOME TEAM
Function BecomeTeam(cyc,v)
If game=1
 ;friends by default
 BecomeFriends(cyc,v,1) 
 ;consistent allegiance
 If charHeel(v)<>charHeel(cyc)
  charHeel(v)=charHeel(cyc)
  If charHeel(v)=0 Then charEyes(v)=2
  If charHeel(v)=1 Then charEyes(v)=1
 EndIf
 ;assign management
 If charManager(cyc)=0 Then charManager(cyc)=v
 If charManager(v)=0 Then charManager(v)=cyc 
EndIf
End Function

;BECOME FRIENDS
Function BecomeFriends(cyc,v,task) ;0=in nobody else, 1=override existing
If game=1
 ;reconsider relationship
 If charEnemy(cyc)=v Then charEnemy(cyc)=0
 If charEnemy(v)=cyc Then charEnemy(v)=0 
 If cyc=gamChar(slot) And gamOpponent(slot,GetDate())=v And screen<>50 Then gamOpponent(slot,GetDate())=0 
 ;assign friends
 oldFriend1=charFriend(cyc) : oldFriend2=charFriend(v) 
 If charFriend(cyc)=0 Or task=1 Then charFriend(cyc)=v
 If charFriend(v)=0 Or task=1 Then charFriend(v)=cyc
 ;risk friction with YOUR old friend
 randy=Rnd(0,1)
 If randy=0 And cyc=gamChar(slot) And oldFriend1>0 And charFriend(cyc)<>oldFriend1
  If gamOpponent(slot,GetDate())=0 And gamPromo(slot,GetDate())=0 
   gamOpponent(slot,GetDate())=oldFriend1
   gamPromo(slot,GetDate())=44
  EndIf
 EndIf
 ;risk friction with THEIR old friend
 If randy=1 And cyc=gamChar(slot) And oldFriend2>0 And charFriend(v)<>oldFriend2
  If gamOpponent(slot,GetDate())=0 And gamPromo(slot,GetDate())=0 
   gamOpponent(slot,GetDate())=oldFriend2
   gamPromo(slot,GetDate())=43
  EndIf
 EndIf
EndIf
End Function

;BECOME ENEMIES
Function BecomeEnemies(cyc,v,task) ;0=if nobody else, 1=override existing
If game=1
 ;reconsider relationship
 If charFriend(cyc)=v Then charFriend(cyc)=0
 If charFriend(v)=cyc Then charFriend(v)=0 
 If charManager(cyc)=v Then charManager(cyc)=0
 If charManager(v)=cyc Then charManager(v)=0 
 ;assign enemies
 If charEnemy(cyc)=0 Or task=1 Then charEnemy(cyc)=v
 If charEnemy(v)=0 Or task=1 Then charEnemy(v)=cyc
EndIf
End Function

;PROMO CHARM EFFECTS
Function PromoCharm(factor)
 pHeat(speaker)=pHeat(speaker)+5
End Function

