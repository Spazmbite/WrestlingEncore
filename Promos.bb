Function DisplayPromo()
 ;font & spacing
 If optGameRes=<1 Then SetFont font(2)
 If optGameRes=2 Then SetFont font(3)
 If optGameRes=>3 Then SetFont font(4)
 r1=250 : g1=250 : b1=250
 x=rX#(400) : y=rY#(505) : s=rY#(36)
 speaker=0
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 1. WHY D'YOU INTERFER?! ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=1
  If promoTim>850 And promoTim<1100
   Speak(promoActor(2),promoActor(1),1)
   Outline("What the hell are you up to, "+pName$(promoActor(1))+"?!",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("Who do you think you are, interfering in MY business?!",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<950 And promoReact(1)=0 Then Pop(0,7,0) : promoReact(1)=1 : PromoCharm(4)
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(2),promoActor(1),1) : PushTurn(promoActor(2),promoActor(1))
   Outline("A guy that messes with my career is either mighty",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("brave or mighty STUPID! Which are you, boy?!",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<750 And promoReact(2)=0 Then Pop(speaker,8,1) : promoReact(2)=1 : PromoCharm(4)
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(1),promoActor(2),1)
   Outline("Hey, i was just evening up the score! You've been",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("stepping out line all too often in recent weeks...",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<435 And promoReact(3)=0 Then Pop(speaker,4,0) : promoReact(3)=1 : PromoCharm(4)
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(2),promoActor(1),1)
   Outline("Yeah, i make people's lives a misery all right - and you",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("just added yourself to the list! Watch your back...",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<175 And promoReact(4)=0 Then Pop(speaker,2,1) : promoReact(4)=1 : PromoCharm(4)
   BecomeEnemies(pChar(promoActor(1)),pChar(promoActor(2)),1)
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 2. GLAD I INTERFERED! ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=2
  If promoTim>850 And promoTim<1100
   Speak(promoActor(2),promoActor(1),2) : PushTurn(promoActor(2),promoActor(1))
   Outline("Hey, "+pName$(promoActor(1))+", i guess you're wondering",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("why i got involved in your match last week?",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<950 And promoReact(1)=0 Then Pop(speaker,2,1) : promoReact(1)=1 : PromoCharm(4)
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(1),promoActor(2),1)
   Outline("To be honest, i don't care what you're up to.",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("I'm just glad i get to deliver the punishment!",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<750 And promoReact(2)=0 Then Pop(speaker,8,1) : promoReact(2)=1 : PromoCharm(4)
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(2),promoActor(1),1)
   Outline("I bring a bit of class to your match by showing my",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("face - and THAT's the thanks i get?! You ingrate!",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<435 And promoReact(3)=0 Then Pop(speaker,2,1) : promoReact(3)=1 : PromoCharm(4)
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(2),promoActor(1),1)
   Outline("I guess i didn't make a good enough impression the",rX#(400),rY#(520),0,0,0,r1,g1,b1)
   Outline("first time, but i'm gonna make a BIG impact right now!",rX#(400),rY#(555),0,0,0,r1,g1,b1)
   If promoTim<175 And promoReact(4)=0 Then Pop(0,6,1) : promoReact(4)=1 : PromoCharm(4)
   BecomeEnemies(pChar(promoActor(1)),pChar(promoActor(2)),1)
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 3. TEAM FORMATION ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=3
  If game=1 And matchTeams=<0 Then promoLogic=0
